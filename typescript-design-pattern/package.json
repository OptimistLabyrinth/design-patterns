{
  "name": "typescript-design-pattern",
  "version": "1.0.0",
  "description": "learn design patterns using typescript on nodejs",
  "repository": "https://github.com/OptimistLabyrinth/design-patterns",
  "author": "OptimistLabyrinth",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:watch": "ts-node-dev --respawn src/index.ts --watch src/**/*",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "clean": "rm -rf dist/"
  },
  "dependencies": {
    "dayjs": "^1.11.7"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc": "^2.0.4",
    "typescript": "^4.9.4"
  }
}
